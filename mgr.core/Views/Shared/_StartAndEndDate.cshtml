
<script type="text/javascript">

    whenReady(function () {

        laydate.render({
            elem: '#startTime'
            , calendar: true
        });
        laydate.render({
            elem: '#endTime'
            , calendar: true
        });
        if (!window.initLayDate) {
            $('#startTime').val('');
            $('#endTime').val('');
        }
      
    });

    
</script>
